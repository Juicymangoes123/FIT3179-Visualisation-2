{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 450,
    "title": "abcdef",
    "projection": {
        "type": "equirectangular"
    },
    "params": [
        {
            "name": "slider",
            "value": 2010,
            "bind": {
                "input": "range",
                "min": 2004,
                "max": 2025,
                "step": 1,
                "name": "Year"
            }
        }
    ],
    "layer": [
        {
            "data": {
                "url": "au-states.topojson",
                "format": {
                    "type": "topojson",
                    "feature": "ne_10m_admin_1_states_provinces"
                }
            },
            "transform": [
                {
                    "lookup": "properties.name",
                    "from": {
                        "data": {
                            "url": "overseas_migration.csv"
                        },
                        "key": "State",
                        "fields": [
                            "State",
                            "percentage",
                            "year",
                            "net_overseas_migration",
                            "Australian_total"
                        ]
                    }
                },
                {
                    "filter": "datum.year==slider"
                }
            ],
            "mark": {
                "type": "geoshape"
            },
            "encoding": {
                "color": {
                    "field": "percentage",
                    "type": "quantitative",
                    "scale": {
                        "type": "threshold",
                        "domain": [
                            0.05,
                            0.10,
                            0.20,
                            0.30
                        ],
                        "range": [
                            "#81d484",
                            "#55a657",
                            "#338035",
                            "#195e1a",
                            "#053b05"
                        ]
                    }
                },
                "tooltip": [
                    {
                        "field": "percentage",
                        "type": "quantitative"
                    },
                    {
                        "field": "State",
                        "type": "nominal"
                    },
                    {
                        "field": "year",
                        "type": "ordinal"
                    }
                ]
            }
        },
        {
            "data": {
                "url": "au-states.topojson",
                "format": {
                    "type": "topojson",
                    "feature": "ne_110m_graticules_30"
                }
            },
            "mark": {
                "type": "geoshape",
                "stroke": "dimgrey"
            }
        }
    ]
}